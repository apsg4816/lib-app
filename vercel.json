{
    "version": 2,
    "builds": [
      {
        "src": "backend/start.sh",
        "use": "@vercel/python",
        "config": { "entrypoint": "backend/start.sh" }
      },
      {
        "src": "frontend/package.json",
        "use": "@vercel/static-build"
      }
    ],
    "routes": [
        
      {
        "src": "/api/(.*)",
        "dest": "/backend/start.sh"
      },
      {
        "src": "/(.*)",
        "dest": "/frontend/$1"
      }
    ]
  }
  
  