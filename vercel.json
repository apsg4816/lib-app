{
    "version": 2,
    "builds": [
      {
        "src": "backend/app.py",
        "use": "@vercel/python"
      },
      {
        "src": "elibrary/package.json",
        "use": "@vercel/static-build"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/backend/app.py"
      },
      {
        "src": "/(.*)",
        "dest": "/elibrary/$1"
      }
    ]
  }
  